{
  "Logging": {
    "LogLevel": {
      "Default": "None",
      "Microsoft": "None",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ElasticApm": {
    "ServerUrls": "http://elastic-apm:8200"
  },
  "ConnectionStrings": {
    "DataContext": "data source={SQL_IP};initial catalog={SQL_DB};persist security info=True;user id={SQL_USER};password={SQL_PASS};multipleactiveresultsets=True;"
  },
  "Config": {
    "PublicRSAKey": "{PUBLIC_RSA_KEY}"
  },
  "MongoConnection": {
    "ConnectionString": "mongodb://{MG_IP}",
    "Database": "ELODIE"
  },
  "RabbitConfig": {
    "Hostname": "rabbitmq",
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": "5672"
  },
  "InternalServices": {
    "UTILS": "http://utils-backend:8080",
    "PORTAL": "http://portal-backend:8080",
    "DMS": "http://dms-backend:8080",
    "AMS": "http://ams-backend:8080",
    "CRM": "http://crm-backend:8080",
    "CMS_EXPORT": "http://cms-export-backend:8080",
    "ES": "http://es-backend:8080"
  }
}